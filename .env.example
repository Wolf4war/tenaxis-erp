VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef

# Development Settings
VITE_USE_EMULATORS=true

# App Settings
VITE_APP_NAME=Tenaxis
VITE_APP_VERSION=1.0.0
